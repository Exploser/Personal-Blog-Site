{
  "version": 2,
  "name": "personal-blog-site",
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "client/build/index.html"
    }
  ]
}

