{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "JWT_SECRET": "@jwt_secret",
    "MONGO_URI": "@mongo_uri",
    "ALLOWED_ORIGINS": "@allowed_origins"
  }
}

